<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NA_cannot_be_by_itself</fullName>
    <active>true</active>
    <errorConditionFormula>AND(INCLUDES(Investment_Type__c , &quot;N/A&quot;),OR(INCLUDES(Investment_Type__c , &quot;Current&quot;),INCLUDES(Investment_Type__c , &quot;Past&quot;),INCLUDES(Investment_Type__c , &quot;Majority&quot;),INCLUDES(Investment_Type__c , &quot;Minority&quot;)))</errorConditionFormula>
    <errorDisplayField>Investment_Type__c</errorDisplayField>
    <errorMessage>N/A needs to be selected by itself</errorMessage>
</ValidationRule>
